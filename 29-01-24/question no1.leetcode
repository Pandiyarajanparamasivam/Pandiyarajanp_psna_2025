class Solution {
    public int[] twoSum(int[] a, int n) {
        int arr[]=new int[2];
        for(int i=0;i<a.length-1;i++)
        {
            for(int j=i+1;j<a.length;j++)
            {
                if(a[i]+a[j]==n)
                {
                    arr[0]=i;
                    arr[1]=j;
                    break;
                }
            }
        }
        return arr;
    }
}
